<table class="customersTable">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Email</th>
            <th>Member Since</th>
        </tr>
    </thead>

    <tbody>

        {{#each result}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.gender}}</td>
            <td>{{{formatDate this.dob}}}</td>
            <td>{{this.email}}</td>
            <td>{{this.member_since}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>